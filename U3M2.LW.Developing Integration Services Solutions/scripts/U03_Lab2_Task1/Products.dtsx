		public void Main()
		{
            // C# code
            String SendMailFrom = Dts.Variables["From"].Value.ToString();
            String SendMailTo = Dts.Variables["To"].Value.ToString();
            String SendMailSubject = Dts.Variables["Subject"].Value.ToString();
            String SendMailBody = Dts.Variables["RecordCount"].Value.ToString();

            try
            {
                MailMessage email = new MailMessage();
                SmtpClient SmtpServer = new SmtpClient("smtp.gmail.com");
                // START
                email.From = new MailAddress(SendMailFrom);
                email.To.Add(SendMailTo);
                email.Subject = SendMailSubject;
                email.Body = SendMailBody;
                //END

                SmtpServer.Port = 587;
                SmtpServer.Credentials = new System.Net.NetworkCredential(SendMailFrom, "vapxbvxrccjtlbkt");
                SmtpServer.EnableSsl = true;

                SmtpServer.Send(email);
